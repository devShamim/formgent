{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "formgent/gdpr",
	"title": "GDPR",
	"category": "formgent",
	"description": "FormGent GDPR field.",
	"textdomain": "formgent",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"default": "gdpr"
		},
		"checked": {
			"type": "string",
			"default": false
		},
		"label_alignment": {
			"type": "string",
			"default": "top"
		},
		"description": {
			"type": "string",
			"default": "I Accept GDPR"
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"block_width": {
			"type": "string",
			"default": "100"
		}
	},
	"supports": {
		"interactivity": true
	},
	"editorScript": "file:index.js",
	"style": "file:style-index.css",
	"editorStyle": "file:index.css",
	"render": "file:render.php"
}
