{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "formgent/long-text",
  "title": "Long Text",
  "category": "formgent",
  "description": "FormGent Long Text field.",
  "textdomain": "formgent",
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "name": {
      "type": "string",
      "default": "long_text"
    },
    "value": {
      "type": "string",
      "default": "Long Text Value"
    },
    "placeholder": {
      "type": "string",
      "default": "Enter Long Text"
    },
    "label": {
      "type": "string",
      "default": "Long Text"
    },
    "label_alignment": {
      "type": "string",
      "default": "left"
    },
    "sub_label": {
      "type": "string",
      "default": "Enter Long Text Here"
    },
    "required": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "interactivity": true
  }
}
