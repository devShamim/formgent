{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "formgent/button",
	"title": "Custom Button",
	"category": "formgent",
	"description": "FormGent Custom Button field.",
	"textdomain": "formgent",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"default": "button"
		},
		"button_text": {
			"type": "string",
			"default": "Button"
		},
		"label_alignment": {
			"type": "string",
			"default": "top"
		},
		"button_style": {
			"type": "string",
			"default": "default"
		},
		"background_color": {
			"type": "string",
			"default": "#000000"
		}
	},
	"supports": {
		"interactivity": true
	},
	"editorScript": "file:index.js",
	"style": "file:style-index.css",
	"editorStyle": "file:index.css",
	"render": "file:render.php"
}
