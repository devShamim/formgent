{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "formgent/name",
	"title": "Name",
	"category": "formgent",
	"description": "FormGent Name field.",
	"textdomain": "formgent",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"default": "names"
		},
		"content": {
			"type": "string",
			"default": "Content for edit"
		},
		"label": {
			"type": "string",
			"default": "Hello World"
		},
		"format": {
			"type": "string",
			"default": "first_last"
		},
		"checkbox": {
			"type": "string",
			"default": "0"
		},
		"switch": {
			"type": "boolean",
			"default": true
		},
		"margin": {
			"type": "string",
			"default": "10px"
		},
		"padding": {
			"type": "string",
			"default": "10px"
		},
		"repeater": {
			"type": "string",
			"default": "[]"
		},
		"toggle_group": {
			"type": "string",
			"default": "left"
		}
	},
	"supports": {
		"interactivity": true
	},
	"editorScript": "formgent/blocks-editor",
	"editorStyle": "formgent/blocks-editor",
	"style": "formgent/blocks-frontend"
}
