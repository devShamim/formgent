{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "formgent/name",
	"title": "Name",
	"category": "formgent",
	"description": "FormGent Name field.",
	"textdomain": "formgent",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"default": "name"
		},
		"block_width": {
			"type": "string",
			"default": "100"
		},
		"label": {
			"type": "string",
			"default": "Name"
		}
	},
	"supports": {
		"interactivity": true
	},
	"editorScript": "file:index.js",
	"style": "file:style-index.css",
	"editorStyle": "file:index.css"
}
